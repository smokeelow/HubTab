{
    "name": "Light Tab",
    "description": "All that you need in one place",
    "version": "0.3",
    "incognito": "split",
    "background": {
        "persistent": false,
        "scripts": ["core/bg.js"]
    },
    "permissions": [
        "tabs",
        "*://*/*"
    ],
    "chrome_url_overrides": {
        "newtab": "templates/tab.html"
    },
    "icons" : {
        "16" : "icon-16.png",
        "48" : "icon-48.png",
        "128" : "icon-128.png"
    },
    "manifest_version": 2
}
