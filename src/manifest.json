{
    "name": "HubTab",
    "description": "All that you need in one place",
    "version": "0.1",
    "incognito": "split",
    "permissions": [
        "bookmarks",
        "tabs",
        "chrome://favicon/*",
        "http://*/*",
        "https://*/*",
        "script-src"
    ],
    "chrome_url_overrides": {
        "newtab": "templates/tab.html"
    },
    "manifest_version": 2
}
